{
  "expo": {
    "name": "AIS Viewer",
    "slug": "ais-viewer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "ais-viewer",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a365d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.orca.aisviewer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a365d"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.orca.aisviewer"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "@rnmapbox/maps",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Show your current location on the map."
        }
      ]
    ]
  }
}
